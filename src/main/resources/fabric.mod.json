{
  "schemaVersion": 1,
  "id": "fzzymapz",
  "version": "${version}",
  "name": "Fzzymapz",
  "description": "A map creation Library that can be used to build custom tile-based maps and mini-maps that record and visualize all sorts of chunk data.",
  "authors": [
    "fzzyhmstrs"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/imbued-ascendancy",
    "issues": "https://github.com/fzzyhmstrs/ia/issues",
    "sources": "https://github.com/fzzyhmstrs/ia"
  },
  "license": "MIT License",
  "icon": "assets/fzzymapz/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.fzzyhmstrs.fzzymapz.FM"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "me.fzzyhmstrs.fzzymapz.FMClient"
      }
    ]
  },
  "mixins": [
    "fzzymapz.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21",
    "minecraft": ">=1.19.3",
    "java": ">=17"
  }
}
